AWS_REGION=<%= @aws_region %>
AWS_SECRET_ACCESS_KEY=<%= @s3_secret_key %>
AWS_ACCESS_KEY_ID=<%= @s3_access_key %>
JAVA_ARGS="-Xmx<%= @heap_indexer_memory_kb %>k -Xms<%= @heap_indexer_memory_kb %>k -XX:+UseConcMarkSweepGC -XX:+PrintGCDetails -XX:+PrintGCTimeStamps -verbose:gc -server -verbose:gc -XX:+PrintGCDetails -XX:+PrintGCTimeStamps -Djava.rmi.server.hostname=<%= @rmi_address %> -Dcom.sun.management.jmxremote -Dcom.sun.management.jmxremote.authenticate=false -Dcom.sun.management.jmxremote.ssl=false -Dcom.sun.management.jmxremote.port=<%= @rmi_port %>"
CONF_DIR=<%= @parent_config_dir %>