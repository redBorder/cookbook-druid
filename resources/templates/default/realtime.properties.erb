# General
druid.service=realtime
druid.server.type=realtime
druid.host=<%= @name %>.node.<%= @cdomain %>
druid.port=<%= @port %>

# Realtime
druid.publish.type=metadata
druid.realtime.specFile=/etc/druid/realtime/rb_realtime.spec

# Query
druid.processing.buffer.sizeBytes=<%= @processing_memory_buffer_b %>
druid.processing.numThreads=<%= @num_threads%>

# Logging
druid.monitoring.monitors=["com.metamx.metrics.JvmMonitor", "io.druid.segment.realtime.RealtimeMetricsMonitor"]
